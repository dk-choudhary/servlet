<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>EmpRecord</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
  <servlet-name>login</servlet-name>
<servlet-class>com.user.UserProfileLogin</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login1</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>signup1</servlet-name>
<servlet-class>com.user.UserSignupServlet</servlet-class>
  </servlet>  
  <servlet-mapping>
  <servlet-name>signup1</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Edit</servlet-name>
<servlet-class>com.user.UserEdit</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Edit</servlet-name>
  <url-pattern>/UserEdit</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Edits</servlet-name>
<servlet-class>com.user.EditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Edits</servlet-name>
  <url-pattern>/ServletEdit</url-pattern>
  </servlet-mapping>
</web-app>